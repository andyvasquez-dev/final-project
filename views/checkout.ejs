                <form class="product-checkout-form">
                  <div class="row">
                    <div class="col-md-6">
                      <h3>Billing Details</h3>
                      <div class="row">
                        <div class="col-md-6">

                          <div class="form-group">

                            <label>First Name</label>
                            <% if(userInfo.first!="omit"){ %>

                              <input type="text" value="<%= userInfo.user%>" class="form-control" name="first">

                            <% } else { %>
                              <input type="text" value="<%= userInfo.user%>" class="form-control" name="first">

                            <% } %>
                          </div>
                        </div>
                        <div class="col-md-6">
                          <div class="form-group">
                            <label>Last Name</label>
                            <% if(userInfo.last!="omit"){ %>
                              <input type="text" placeholder="" class="form-control" name="last" value="<%= userInfo.last %>">

                            <% } else { %>
                              <input type="text" placeholder="" class="form-control" name="last">

                            <% } %>
                          </div>
                        </div>

                        <div class="col-md-12">
                          <div class="form-group">
                            <label>Street Address</label>
                            <!-- street address -->
                            <% if(userInfo.address!= "omit" ){ %>
                              <input type="text" placeholder="Address" class="form-control mbm" name="address"  value="<%=userInfo.address %>">

                            <% } else { %>
                              <input type="text" placeholder="Address" class="form-control mbm" name="address"  value="">

                            <% } %>

                            <!--  apartment -->
                            <% if(userInfo.apartment!="omit"){ %>
                              <input type="text" placeholder="Apartment, suit, unit etc." class="form-control" name="apartment" value="<%= userInfo.apartment%>">

                            <% } else { %>
                              <input type="text" pl3ceholder="Apartment, suit, unit etc." class="form-control" name="apartment">

                            <% } %>

                          </div>
                        </div>
                        <div class="col-md-12">
                          <div class="form-group">
                            <label>Town / City</label>
                            <% if(userInfo.city!="omit"){ %>
                              <input type="text" placeholder="" value="<%= userInfo.city%>"class="form-control" name="city">
                            <% } else { %>
                              <input type="text" placeholder=""  class="form-control" name="city">
                            <% } %>
                          </div>
                        </div>
                        <div class="col-md-12">
                          <div class="form-group">
                            <label>State / Country</label>
                            <% if( userInfo.state != "omit" ){ %>
                              <input type="text" placeholder="" class="form-control" name="state" value="<%=userInfo.state%>">

                            <% } else { %>
                              <input type="text" placeholder="" class="form-control" name="state">

                            <% } %>
                          </div>
                        </div>
                        <div class="col-md-12">
                          <div class="form-group">
                            <label>Postcode / ZIP</label>
                            <% if(userInfo.zip != "omit"){ %>
                              <input type="text" placeholder="" class="form-control" name="zip" value="<%= userInfo.zip%>">

                            <% } else { %>
                              <input type="text" placeholder="" class="form-control" name="zip">

                             <%}%>
                          </div>
                        </div>
                        <div class="col-md-6">
                          <div class="form-group">
                            <label>Phone</label>
                            <% if(userInfo.phone != "omit"){ %>
                              <input type="text" value="<%= userInfo.user%>" class="form-control" name="phone">

                            </div>
                              <% } else { %>
                                <input type="text" name="phone" class="form-control">
                            <%  } %>
                        </div>
                        <div class="col-md-6">
                          <div class="form-group">
                            <label>Email Address</label>
                              <input type="text" placeholder="" class="form-control" name="email" value="<%=user.local.email %>">
                          </div>
                        </div>
                      </div>
                    </div>

                  </div>
                  <div class="row">
                    <div class="col-md-12">
                      <h3>Your order</h3>
                      <table class="shop_table">
                        <thead>
                          <tr>
                            <th class="product-name">Product</th>
                            <th class="product-total">Total</th>
                          </tr>
                        </thead>
                        <tbody>
                          <% for(var i = 0; i < order.order.length; i++){ %>
                            <tr class="cart_item">
                              <td class="product-name"><%= order.order[i].dish%> 							 <strong class="product-quantity">× <%= order.order[i].quantity%></strong></td>
                              <td class="product-total"><span class="woocommerce-Price-amount amount"><span class="woocommerce-Price-currencySymbol">$</span><%= order.order[i].dishTotal%></span></td>
                            </tr>
                          <% } %>


                        </tbody>
                        <tfoot>
                          <tr class="cart-subtotal">
                            <th>Subtotal</th>
                            <td><span class="woocommerce-Price-amount amount"><span class="woocommerce-Price-currencySymbol">$</span><%= order.total%></span></td>
                          </tr>
                          <tr class="order-total">
                            <th>Total</th>
                            <td><strong><span class="woocommerce-Price-amount amount"><span class="woocommerce-Price-currencySymbol">$</span><%= order.total%></span></strong></td>
                          </tr>
                        </tfoot>
                      </table>
                    </div>
                    <div class="col-md-12">
                      <div class="checkout-product-options">
                        <h4>Checkout Method</h4>
                        <% if (userInfo.payment != "omit") { %>
                            <% if(userInfo.payment==="paypal"){ %>
                                <div class="form-check">
                                  <input id="productQuantity1" type="radio" name="payment" value="credit" class="form-check-input">
                                  <label for="productQuantity1" class="form-check-label"> Credit Card</label>
                                </div>
                                <div class="form-check">
                                  <input id="productQuantity3" type="radio" name="payment" value="paypal" checked class="form-check-input">
                                  <label for="productQuantity3" class="form-check-label">Paypal</label>
                                </div>
                            <% } else { %>
                                <div class="form-check">
                                  <input id="productQuantity1" type="radio" name="payment" value="credit" checked class="form-check-input">
                                  <label for="productQuantity1" class="form-check-label"> Credit Card</label>
                                </div>
                                <div class="form-check">
                                  <input id="productQuantity3" type="radio" name="payment" value="paypal"  class="form-check-input">
                                  <label for="productQuantity3" class="form-check-label">Paypal</label>
                                </div>
                            <% } %>
                        <% } else { %>
                          <div class="form-check">
                            <input id="productQuantity1" type="radio" name="payment" value="credit" class="form-check-input">
                            <label for="productQuantity1" class="form-check-label"> Credit Card</label>
                          </div>
                          <div class="form-check">
                            <input id="productQuantity3" type="radio" name="payment" value="paypal" class="form-check-input">
                            <label for="productQuantity3" class="form-check-label">Paypal</label>
                          </div>
                        <% } %>
                      </div>
                    </div>
                    <div class="col-md-12">    <!-- infoButton id below -->
                      <p class="hidden orderID"><%=order._id%></p>
                      <a class="swin-btn checkout-submit butt" id="submitButton" href="javascript:void(0)"><span>Place Order</span></a>
                    </div>
                  </div>
                </form>
              </section>
