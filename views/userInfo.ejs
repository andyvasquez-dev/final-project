                      <div class="row">
                        <div class="col-md-6">
                          <div class="form-group">
                            <label>First Name</label>
                            <input type="text" placeholder="" name="first" class="form-control">
                          </div>
                        </div>
                        <div class="col-md-6">
                          <div class="form-group">
                            <label>Last Name</label>
                            <input type="text" placeholder="" name="last" class="form-control">
                          </div>
                        </div>
                        <div class="col-md-12">
                          <div class="form-group">
                            <label>Call Name ( in case we call you at the restaurant )</label>
                            <input type="text" placeholder="Call name" class="form-control mbm" name="callName">
                          </div>
                        </div>
                        <div class="col-md-12">
                          <div class="form-group">
                            <label>Street Address</label>
                            <input type="text" placeholder="Address" class="form-control mbm" name="address">
                            <input type="text" placeholder="Apartment, suit, unit etc." class="form-control" name="apartment">
                          </div>
                        </div>
                        <div class="col-md-12">
                          <div class="form-group">
                            <label>Town / City</label>
                            <input type="text" placeholder="" name="city" class="form-control">
                          </div>
                        </div>
                        <div class="col-md-12">
                          <div class="form-group">
                            <label>State</label>
                            <input type="text" placeholder="" name="state" class="form-control">
                          </div>
                        </div>
                        <div class="col-md-12">
                          <div class="form-group">
                            <label>Postcode / ZIP</label>
                            <input type="text" placeholder="" name="zip" class="form-control">
                          </div>
                        </div>
                        <div class="col-md-6">
                          <div class="form-group">
                            <label>Phone</label>
                            <input type="text" placeholder="" name="phone" class="form-control">
                          </div>
                        </div>
                      </div>
                    </div>

                  </div>
                  <div class="row">
                    <div class="col-md-12">
                      <div class="checkout-product-options">
                        <h4>Preferred Payment Method for Checkout?</h4>
                        <div class="form-check">
                          <input id="productQuantity1" type="radio" name="payment" value="credit" class="form-check-input">
                          <label for="productQuantity1" class="form-check-label">Credit Card</label>
                        </div>
                        <div class="form-check">
                          <input id="productQuantity3" type="radio" name="payment" value="paypal" class="form-check-input">
                          <label for="productQuantity3" class="form-check-label">Paypal</label>
                        </div>
                      </div>
                    </div>

